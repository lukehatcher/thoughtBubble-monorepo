{
  "name": "test",
  "description": "can we get react into vscode extension?",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:thoughtBubble.start",
    "onCommand:thoughtBubble.kill",
    "onCommand:thoughtBubble.login"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-extension-samples.git"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "thoughtBubble.start",
        "title": "view thoughts",
        "category": "thoughtBubble"
      },
      {
        "command": "thoughtBubble.kill",
        "title": "close",
        "category": "thoughtBubble"
      },
      {
        "command": "thoughtBubble.login",
        "title": "login",
        "category": "thoughtBubble"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run bundle-view && npm run bundle-ext",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch-ext": "webpack --config webpack.config.js --mode development -w",
    "bundle-ext": "webpack --config webpack.config.js --mode development",
    "watch-view": "webpack --config webpack.view.config.js --mode development -w",
    "bundle-view": "webpack --config webpack.view.config.js --mode development"
  },
  "dependencies": {
    "@octokit/rest": "^18.3.5",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.2",
    "reactjs-popup": "^2.0.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "typescript": "^4.0.2",
    "@types/node": "^12.12.0",
    "@types/react": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-dom": "^17.0.1",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "ts-loader": "^8.0.17",
    "redux-devtools-extension": "^2.13.9",
    "css-loader": "^5.1.1",
    "style-loader": "^2.0.0",
    "eslint": "^7.22.0",
    "prettier": "2.2.1",
    "eslint-config-prettier": "^8.1.0",
    "supports-color": "^8.1.1"
  }
}
